<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Bluewin user login request pattern</Name>
     <Description>Login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Bluewin</Company>
   <WebPage>www.bluewin.ch</WebPage>
   <Method>POST</Method>
   <Host>login\.sso\.bluewin\.ch</Host>
   <Path>\/login\?.*</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>