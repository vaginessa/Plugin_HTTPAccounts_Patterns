<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Galaxus user login request pattern</Name>
     <Description>Galaxus user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Galaxus</Company>
   <WebPage>www.galaxus.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.galaxus\.ch</Host>
   <Path>.*CustomerAccount\/Login.*</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>