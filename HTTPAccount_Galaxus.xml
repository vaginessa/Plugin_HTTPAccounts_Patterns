<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Galaxus login request pattern" description="Galaxus login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Galaxus</Company>
   <WebPage>www.galaxus.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.galaxus\.ch</Host>
   <Path>.*CustomerAccount\/Login.*</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>