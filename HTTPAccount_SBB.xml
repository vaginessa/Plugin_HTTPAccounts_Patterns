<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="SBB login request pattern" description="SBB login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>SBB</Company>
   <WebPage>www.sbb.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.sbb\.ch</Host>
   <Path>\/ticketshop\/b2c\/logon\.do\?</Path>
   <DataPattern>logon\.username=([^\s&amp;=]+)&amp;logon\.password=([^\s\n\r&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>