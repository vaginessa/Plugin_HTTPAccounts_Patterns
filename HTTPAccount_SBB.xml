<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>SBB user login request pattern</Name>
     <Description>SBB user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>SBB</Company>
   <WebPage>www.sbb.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.sbb\.ch</Host>
   <Path>\/ticketshop\/b2c\/logon\.do\?</Path>
   <DataPattern>logon\.username=([^\s&amp;=]+)&amp;logon\.password=([^\s\n\r&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>