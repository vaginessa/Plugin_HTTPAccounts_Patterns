<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Zalando.ch login request pattern" description="Zalando.ch login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Zalando</Company>
   <WebPage>www.zalando.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.zalando\.ch</Host>
   <Path>\/login</Path>
   <DataPattern>email=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>