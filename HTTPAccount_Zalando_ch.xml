<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Zalando.ch user login request pattern</Name>
     <Description>Zalando.ch user login request pattern for URL</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Zalando</Company>
   <WebPage>www.zalando.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.zalando\.ch</Host>
   <Path>\/login</Path>
   <DataPattern>email=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>