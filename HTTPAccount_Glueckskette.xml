<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Glückskette user login request pattern</Name>
     <Description>Glückskette user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Glueckskette</Company>
   <WebPage>www.glueckskette.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.glueckskette\.ch</Host>
   <Path>.*spenden\/login-registrierte-benutzer\.html</Path>
   <DataPattern>user=([^\s&amp;=]+)&amp;pass=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>