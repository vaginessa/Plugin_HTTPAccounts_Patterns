<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Glueckskette login request pattern" description="Glueckskette login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Glueckskette</Company>
   <WebPage>www.glueckskette.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.glueckskette\.ch</Host>
   <Path>.*spenden\/login-registrierte-benutzer\.html</Path>
   <DataPattern>user=([^\s&amp;=]+)&amp;pass=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>