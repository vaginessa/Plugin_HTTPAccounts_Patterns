<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Dailymotion login request pattern" description="Dailymotion login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Dailymotion</Company>
   <WebPage>www.dailymotion.com</WebPage>
   <Method>POST</Method>
   <Host>dailymotion\.com</Host>
   <Path>\/pageitem\/login</Path>
   <DataPattern>dm_pageitem_login&amp;username=([^\s&amp;]+)&amp;password=([^\s&amp;]+)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>