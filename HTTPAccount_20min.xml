<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="20min.ch login request pattern" description="20min.ch login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>20min</Company>
   <WebPage>www.20min.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.20min\.ch</Host>
   <Path>\/log\/in</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>