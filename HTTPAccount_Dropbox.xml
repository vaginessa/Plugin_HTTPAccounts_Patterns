<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Dropbox login request pattern" description="Dropbox login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Dropbox</Company>
   <WebPage>www.dropbox.com</WebPage>
   <Method>POST</Method>
   <Host>dropbox\.com</Host>
   <Path>.+</Path>
   <DataPattern>login_email=([^\s&amp;=]+)&amp;.*login_password=([^\s&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>