<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Swisslos user login request pattern</Name>
     <Description>Swisslos user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Swisslos</Company>
   <WebPage>www.swisslos.ch</WebPage>
   <Method>POST</Method>
   <Host>.*swisslos\.ch</Host>
   <Path>\/login\/nestedloginswitch\.do</Path>
   <DataPattern>loginName=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>