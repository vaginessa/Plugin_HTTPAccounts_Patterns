<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Swisslos login request pattern" description="Swisslos login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Swisslos</Company>
   <WebPage>www.swisslos.ch</WebPage>
   <Method>POST</Method>
   <Host>.*swisslos\.ch</Host>
   <Path>\/login\/nestedloginswitch\.do</Path>
   <DataPattern>loginName=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>