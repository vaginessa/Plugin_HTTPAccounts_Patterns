<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Search.ch login request pattern" description="Search.ch login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Search</Company>
   <WebPage>www.search.ch</WebPage>
   <Method>POST</Method>
   <Host>login\.search\.ch</Host>
   <Path>\/login</Path>
   <DataPattern>LoginName=([^\s&amp;=]+)&amp;Password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>