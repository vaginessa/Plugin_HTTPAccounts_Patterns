<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Search.ch user login request pattern</Name>
     <Description>Search.ch user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Search</Company>
   <WebPage>www.search.ch</WebPage>
   <Method>POST</Method>
   <Host>login\.search\.ch</Host>
   <Path>\/login</Path>
   <DataPattern>LoginName=([^\s&amp;=]+)&amp;Password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>