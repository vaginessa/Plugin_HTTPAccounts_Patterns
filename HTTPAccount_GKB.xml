<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Graubündner Kantonalbank online banking login request pattern</Name>
     <Description>Graubündner Kantonalbank online banking login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>GKB</Company>
   <WebPage>www.gkb.ch</WebPage>
   <Method>POST</Method>
   <Host>e\-banking\.gkb\.ch</Host>
   <Path>\/WEB_XSA_LOGIN\/</Path>
   <DataPattern>usafe-userid-input=([^\s&amp;=]+)&amp;.+usafe-password-input=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>