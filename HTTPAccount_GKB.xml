<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="GKB login request pattern" description="GKB login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>GKB</Company>
   <WebPage>www.gkb.ch</WebPage>
   <Method>POST</Method>
   <Host>e\-banking\.gkb\.ch</Host>
   <Path>\/WEB_XSA_LOGIN\/</Path>
   <DataPattern>usafe-userid-input=([^\s&amp;=]+)&amp;.+usafe-password-input=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>