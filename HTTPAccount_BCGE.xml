<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>www.bcge.ch login request pattern</Name>
     <Description>www.bcge.ch login request pattern for URL</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>BCGE</Company>
   <WebPage>www.bcge.ch</WebPage>
   <Method>POST</Method>
   <Host>netbanking\.bcge\.ch</Host>
   <Path>login</Path>
   <DataPattern>USERNAME=([^\s&amp;=]+)&amp;PASSWORD=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>