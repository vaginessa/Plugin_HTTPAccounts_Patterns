<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Spotify login request pattern" description="Spotify login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Spotify</Company>
   <WebPage>www.spotify.com</WebPage>
   <Method>POST</Method>
   <Host>spotify\.com</Host>
   <Path>.+</Path>
   <DataPattern>user_name=([^\s&amp;=]+)&amp;.*password=([^\s&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>