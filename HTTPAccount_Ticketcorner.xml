<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Ticketcorner user login request pattern</Name>
     <Description>Ticketcorner user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Ticketcorner</Company>
   <WebPage>www.ticketcorner.ch</WebPage>
   <Method>POST</Method>
   <Host>secure\.ticketcorner\.ch</Host>
   <Path>\/tnl\.dll</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>