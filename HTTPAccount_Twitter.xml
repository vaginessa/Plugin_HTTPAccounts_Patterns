<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Twitter login request pattern" description="Twitter login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Twitter</Company>
   <WebPage>www.twitter.com</WebPage>
   <Method>POST</Method>
   <Host>twitter\.com</Host>
   <Path>.+</Path>
   <DataPattern>username_or_email.*?=([^\s&amp;=]+)&amp;.*password.*?=([^\s&amp;=]+)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>