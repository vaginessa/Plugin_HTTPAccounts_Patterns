<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Salt user login request pattern</Name>
     <Description>Salt user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Salt</Company>
   <WebPage>www.salt.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.salt\.ch</Host>
   <Path>.*\/auth/login\/</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>