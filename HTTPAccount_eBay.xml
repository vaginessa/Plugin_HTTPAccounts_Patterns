<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="eBay login request pattern" description="eBay login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>eBay</Company>
   <WebPage>www.ebay.com</WebPage>
   <Method>POST</Method>
   <Host>signin\.ebay\.com</Host>
   <Path>\/ws\/eBayISAPI\.dll</Path>
   <DataPattern>userid=([^&amp;=\s]+)&amp;pass=([^&amp;=\s]+)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>