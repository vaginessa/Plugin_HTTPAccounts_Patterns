<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Local.ch login request pattern" description="Local.ch login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Local</Company>
   <WebPage>www.local.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.local\.ch</Host>
   <Path>auth\.local\.ch</Path>
   <DataPattern>\/login\.do||j_username=([^\s&amp;=]+)&amp;j_password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>