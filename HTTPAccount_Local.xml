<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Local.ch user login request pattern</Name>
     <Description>Local.ch user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Local</Company>
   <WebPage>www.local.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.local\.ch</Host>
   <Path>auth\.local\.ch</Path>
   <DataPattern>\/login\.do||j_username=([^\s&amp;=]+)&amp;j_password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>