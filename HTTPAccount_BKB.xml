<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Basler Kantonalbank online banking login request pattern</Name>
     <Description>Login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>BKB</Company>
   <WebPage>www.bkb.ch</WebPage>
   <Method>POST</Method>
   <Host>ebanking\.bkb\.ch</Host>
   <Path>\/ebankingLogin10\/login</Path>
   <DataPattern>contractid=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>