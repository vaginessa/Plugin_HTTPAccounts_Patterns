<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Sunrise user login request pattern</Name>
     <Description>Sunrise user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Sunrise</Company>
   <WebPage>www1.sunrise.ch</WebPage>
   <Method>POST</Method>
   <Host>mip\.sunrise\.ch</Host>
   <Path>\/mip\/dyn\/login\/login\?.*</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>