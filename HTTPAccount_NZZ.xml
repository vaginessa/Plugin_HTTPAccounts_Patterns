<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Neue Zürcher Zeitung user login request pattern</Name>
     <Description>Neue Zürcher Zeitung user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>NZZ</Company>
   <WebPage>www.nzz.ch</WebPage>
   <Method>POST</Method>
   <Host>login\.nzz\.ch</Host>
   <Path>.*</Path>
   <DataPattern>loginName=([^\s&amp;=]+)&amp;loginPass=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>