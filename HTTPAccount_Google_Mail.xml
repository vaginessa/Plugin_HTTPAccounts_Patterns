<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Google mail login request pattern" description="Google mail login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Google mail</Company>
   <WebPage>accounts.google.com</WebPage>
   <Method>POST</Method>
   <Host>accounts\.google\.com</Host>
   <Path>\//AccountLoginInfo</Path>
   <DataPattern>&amp;Email=([^\s&amp;=]+)&amp;Passwd=([^\s&amp;=]+)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>