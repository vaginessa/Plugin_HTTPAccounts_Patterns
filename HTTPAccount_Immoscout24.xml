<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Immoscout24 user login request pattern</Name>
     <Description>Immoscout24 user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Immoscout24</Company>
   <WebPage>www.immoscout24.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.immoscout24\.ch</Host>
   <Path>\/Private\/Login\/Login</Path>
   <DataPattern>LoginDataModel\.LoginName=([^\s&amp;=]+)&amp;LoginDataModel\.Password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>