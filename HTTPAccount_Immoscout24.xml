<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Immoscout24 login request pattern" description="Immoscout24 login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Immoscout24</Company>
   <WebPage>www.immoscout24.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.immoscout24\.ch</Host>
   <Path>\/Private\/Login\/Login</Path>
   <DataPattern>LoginDataModel\.LoginName=([^\s&amp;=]+)&amp;LoginDataModel\.Password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>