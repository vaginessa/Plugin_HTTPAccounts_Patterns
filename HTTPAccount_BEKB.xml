<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="BEKB login request pattern" description="BEKB login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>BEKB</Company>
   <WebPage>www.bekb.ch</WebPage>
   <Method>POST</Method>
   <Host>banking\.bekb\.ch</Host>
   <Path>flow\/login</Path>
   <DataPattern>contractNo=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>