<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Berner Kantonalbank online banking login request pattern</Name>
     <Description>Login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>BEKB</Company>
   <WebPage>www.bekb.ch</WebPage>
   <Method>POST</Method>
   <Host>banking\.bekb\.ch</Host>
   <Path>flow\/login</Path>
   <DataPattern>contractNo=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>