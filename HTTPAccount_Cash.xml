<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Cash login request pattern" description="Cash login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Cash</Company>
   <WebPage>www.cash.ch</WebPage>
   <Method>POST</Method>
   <Host>ebanking\.cash\.ch</Host>
   <Path>\/ebankingLogin\/login</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password||SX=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>