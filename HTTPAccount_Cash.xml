<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>www.cash.ch user login request pattern</Name>
     <Description>Cash user login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Cash</Company>
   <WebPage>www.cash.ch</WebPage>
   <Method>POST</Method>
   <Host>ebanking\.cash\.ch</Host>
   <Path>\/ebankingLogin\/login</Path>
   <DataPattern>username=([^\s&amp;=]+)&amp;password||SX=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>