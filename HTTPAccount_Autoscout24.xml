<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>www.autoscout24.ch login request pattern</Name>
     <Description>www.autoscout24.ch login request pattern for URL</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Autoscout24</Company>
   <WebPage>www.autoscout24.ch</WebPage>
   <Method>POST</Method>
   <Host>www\.autoscout24\.ch</Host>
   <Path>.*account\/logon</Path>
   <DataPattern>LogOnModel\.UserName=([^\s&amp;=]+)&amp;LogOnModel\.Password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>