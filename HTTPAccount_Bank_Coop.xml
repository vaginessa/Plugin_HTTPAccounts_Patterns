<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config>
     <Name>Online banking bankcoop.ch login request pattern</Name>
     <Description>www.bankcoop.ch login request pattern for [URL]</Description>
     <Timestamp>2016-02-03</Timestamp>
     <Author>Simsang</Author>
     <Reference>http://buglist.io/ </Reference>
   </Config>
   
   <Company>Bank-Coop</Company>
   <WebPage>www.bankcoop.ch</WebPage>
   <Method>POST</Method>
   <Host>onlinebanking\.bankcoop\.ch</Host>
   <Path>\/ebankingLogin10\/login</Path>
   <DataPattern>contractid=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>