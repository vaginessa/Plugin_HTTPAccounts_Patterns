<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Bank Coop login request pattern" description="Bank Coop login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Bank-Coop</Company>
   <WebPage>www.bankcoop.ch</WebPage>
   <Method>POST</Method>
   <Host>onlinebanking\.bankcoop\.ch</Host>
   <Path>\/ebankingLogin10\/login</Path>
   <DataPattern>contractid=([^\s&amp;=]+)&amp;password=([^\s\n\r&amp;=]*?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>