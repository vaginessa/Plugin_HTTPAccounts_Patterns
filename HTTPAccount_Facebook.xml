<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Facebook login request pattern" description="Facebook login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Facebook</Company>
   <WebPage>www.facebook.com</WebPage>
   <Method>POST</Method>
   <Host>facebook\.com</Host>
   <Path>.+</Path>
   <DataPattern>email=([^\s&amp;=]+)&amp;pass=([^\s\n\r&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>