<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Wordpress CMS login request pattern" description="Wordpress CMS login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>Wordpress CMS login</Company>
   <WebPage>www.wordpress.com</WebPage>
   <Method>POST</Method>
   <Host>.+</Host>
   <Path>wp-login\.php</Path>
   <DataPattern>log=([^\s&amp;=]+)&amp;pwd=([^\s&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>