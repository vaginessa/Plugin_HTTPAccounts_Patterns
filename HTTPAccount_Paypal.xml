<?xml version="1.0" ?> 
<HTTPAccountPattern>
   <Config name="Paypal login request pattern" description="Paypal login request pattern for URL" date="2016-02-03">
     <Author name="Simsang" reference="http://buglist.io/" />
   </Config>
   
   <Company>PayPal</Company>
   <WebPage>www.paypal.com</WebPage>
   <Method>POST</Method>
   <Host>paypal\.com</Host>
   <Path>.+</Path>
   <DataPattern>login_email=([^\s&amp;=]+)&amp;.*login_password=([^\s&amp;=]+?)(&amp;|\.\.|\s|$)</DataPattern>
</HTTPAccountPattern>